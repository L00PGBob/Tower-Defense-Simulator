local Window = Rayfield:CreateWindow({
	Name = "Tower Defense Simulator | @GenericBob#6126",
	LoadingTitle = "TDS Hub | @GenericBob#6126 ",
	LoadingSubtitle = "by Loop / GenericBob",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = TDSHub, -- Create a custom folder for your hub/game
		FileName = "Cfg"
	},
        Discord = {
        	Enabled = true,
        	Invite = "https://discord.gg/WuTDgwhEYn", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "TDS Bub",
		Subtitle = "Key System",
		Note = "Key = ILoveLoop",
		FileName = "Loop Key",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "ILoveLoop"
	}
})





